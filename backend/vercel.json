{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/ingest?secret=${CRON_SECRET}",
      "schedule": "0 6 * * *"
    }
  ]
}

